<h1>Redirection list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Sourceurl</th>
            <th>Destinationurl</th>
            <th>Httpcode</th>
            <th>Id</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('redirection_show', { 'id': entity.id }) }}">{{ entity.sourceUrl }}</a></td>
            <td>{{ entity.destinationUrl }}</td>
            <td>{{ entity.httpCode }}</td>
            <td>{{ entity.id }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('redirection_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('redirection_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('redirection_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
